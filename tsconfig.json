{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015"],
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@feathersjs/feathers": [
        "node_modules/@types/feathersjs__feathers"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
